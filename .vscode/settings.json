{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "out": true,
    "dist": true
  },
  "search.exclude": {
    "out": true,
    "dist": true
  },
  "typescript.tsc.autoDetect": "off",
  "hide-files.files": [],
  "tsxRunner": {
    "hostSpecifier": "123"
  },
  "ae-tsx-runner": {
    "input": "dist/index.tsx",
    "output": "dist/index.jsx"
  }
}