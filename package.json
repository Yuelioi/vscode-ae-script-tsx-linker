{
    "name": "ae-tsx-runner",
    "displayName": "Adobe AE jsx && tsx Runner",
    "description": "Run Adobe After Effects jsx or tsx Script without ExtendScript Toolkit.",
    "version": "0.6.0",
    "publisher": "yuelili",
    "repository": {
        "type": "git",
        "url": "https://github.com/Yuelioi/vscode-ae-script-tsx-linker.git"
    },
    "engines": {
        "vscode": "^1.75.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:runrun.JSXScript"
    ],
    "keywords": [
        "ae",
        "after effects",
        "extendscript",
        "jsx",
        "tsx",
        "runner"
    ],
    "icon": "img/icon.png",
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "runrun.JSXScript",
                "title": "运行 AE 脚本",
                "icon": {
                    "light": "./img/light.svg",
                    "dark": "./img/dark.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceExtname == .jsx || resourceExtname == .tsx || resourceExtname == .jsxbin",
                    "command": "runrun.JSXScript",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "resourceExtname == .jsx || resourceExtname == .tsx || resourceExtname == .jsxbin",
                    "command": "runrun.JSXScript",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "when": "resourceExtname == .jsx || resourceExtname == .tsx || resourceExtname == .jsxbin",
                    "command": "runrun.JSXScript",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "package2": "vsce package",
        "publish": "vsce publish",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.75.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "@vscode/test-electron": "^2.1.5",
        "del": "^7.0.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.19",
        "run-sequence": "^2.2.1",
        "ts-loader": "^9.3.1",
        "typescript": "^4.9.5",
        "vscode-nls-dev": "^4.0.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "fs-extra": "^10.1.0",
        "vscode-nls": "^5.2.0"
    },
    "license": "MIT",
    "homepage": "https://github.com/Yuelioi/vscode-ae-script-tsx-linker"
}